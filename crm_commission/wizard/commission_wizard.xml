<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="commission_wizard_form" model="ir.ui.view">
            <field name="name">Commission wizard</field>
            <field name="model">commission_wizard</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <group>
                                <field name="from_date"/>
                                <field name="to_date"/>
                            </group>
                            <group>
                                <field name="team_ids" widget="many2many_tags"/>
                                <field name="user_ids" widget="many2many_tags"/>
                            </group>
                        </group>
                    </sheet>
                    <footer>
                        <button name="action_report" string="Print excel Report" type="object" class="btn-primary"/>
                        <button name="action_report_pdf" string="Print PDF Report" type="object" class="btn-primary"/>
                        <button special="cancel" string="Cancel" class="btn-secondary"/>
                    </footer>
                </form>
            </field>
        </record>

        <act_window id="commission_wizard"
                    name="Report"
                    res_model="commission_wizard"
                    view_mode="form"
                    view_id="commission_wizard_form"
                    target="new"/>

    </data>
</odoo>